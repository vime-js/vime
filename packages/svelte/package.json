{
  "name": "@vime/svelte",
  "version": "5.0.19",
  "description": "Svelte bindings for the Vime media player.",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "svelte": "src/svelte.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "contributors": [
    "Rahim Alwer <rahim_alwer@outlook.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vime-js/vime.git"
  },
  "bugs": {
    "url": "https://github.com/vime-js/vime/issues"
  },
  "scripts": {
    "build": "rimraf dist && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es6 --outDir dist/esm",
    "serve": "snowpack dev"
  },
  "dependencies": {
    "@vime/core": "^5.0.19"
  },
  "devDependencies": {
    "@snowpack/plugin-svelte": "^3.4.1",
    "@snowpack/plugin-typescript": "^1.1.1",
    "rimraf": "^3.0.2",
    "snowpack": "^2.18.5",
    "svelte": "^3.24.1",
    "svelte-preprocess": "^4.6.1",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "svelte": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vime",
    "player",
    "media",
    "svelte",
    "audio",
    "video",
    "html5",
    "youtube",
    "dailymotion",
    "vimeo",
    "hls",
    "dash",
    "custom",
    "extensible"
  ]
}
