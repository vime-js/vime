{
  "projectId": "ksvngs",
  "baseUrl": "http://localhost:3334/tests",
  "watchForFileChanges": false,
  "integrationFolder": "cypress/tests",
  "testFiles": "**/*.e2e.ts",
  "fixturesFolder": false,
  "supportsFolder": "cypress/support/index.ts",
  "video": false,
  "screenshotOnRunFailure": false,
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "env": {
    "cypress-plugin-snapshots": {
      "imageConfig": {
        "threshold": 0.3
      }
    }
  },
  "retries": {
    "runMode": 3,
    "openMode": 0
  }
}
