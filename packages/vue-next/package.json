{
  "name": "@vime/vue-next",
  "version": "5.0.34",
  "description": "Vue 3 bindings for the Vime media player.",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vime-js/vime.git"
  },
  "bugs": {
    "url": "https://github.com/vime-js/vime/issues"
  },
  "scripts": {
    "build": "run-s clean:build build:cjs build:esm",
    "build:cjs": "tsc --target es5 --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --outDir dist/esm",
    "build:watch": "npm run build:esm -- --watch --incremental",
    "clean": "rimraf dist src/components",
    "clean:build": "rimraf dist"
  },
  "dependencies": {
    "@vime/core": "^5.0.34"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vime",
    "player",
    "media",
    "vue",
    "vue 3",
    "vue next",
    "audio",
    "video",
    "html5",
    "stenciljs",
    "youtube",
    "dailymotion",
    "vimeo",
    "hls",
    "dash",
    "custom"
  ]
}
